<widget name="sliderbar" type="widget" adapter="ASliderBarAdapter"
            icon="widgets/slider.png" >

     <categories>
     <category name = "basic"/>
     <category name = "title"/>
     <category name = "number"/>
     <category name = "unit"/>
     <category name = "slider"/>
    
     </categories>

