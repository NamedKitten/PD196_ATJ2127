<widget name="listBox" type="widget" adapter="AListBoxAdapter"
            icon="widgets/list.png" >

     <categories>
     <category name = "basic"/>
     <category name = "item"/>
     <category name = "slider"/>
     <category name = "title"/>
     <category name = "ratio"/>
     </categories>
     
