<widget name="progress" type="widget" adapter="AImageAdapter"
			icon="widgets/picregion.png" >

    
	<WidgetbgPic>
	     <property name="name" type="String" value="bg.bmp"  desc="截图的图片名"/>
		<property name="x" type="U16" value="0"  desc="相对控件矩形框左上角x坐标"/>
		<property name="y" type="U16" value="0"  desc="相对控件矩形框右上角y坐标"/>
		<property name="width" type="U16" value="@width"  desc="图片宽度"/>
		<property name="height" type="U16" value="@height"  desc="图片高度"/>
           <property name="picType"  type="Enum"  value="2" enumKeys="非嵌入式背景图,嵌入式背景图,实时子背景图" enumValues="0,1,2" default="2" desc="图片类型"/>
	</WidgetbgPic>
	
	<bgPic>
	     <property name="name" type="String" value="pro_bg.bmp"  desc="截图的图片名"/>
		<property name="x" type="U16" value="@bgPicX"  desc="相对控件矩形框左上角x坐标"/>
		<property name="y" type="U16" value="@bgPicY"  desc="相对控件矩形框右上角y坐标"/>
		<property name="width" type="U16" value="@bgPicWidth"  desc="图片宽度"/>
		<property name="height" type="U16" value="@bgPicHeight"  desc="图片高度"/>
           <property name="picType"  type="Enum"  value="0" enumKeys="非嵌入式背景图,嵌入式背景图,实时子背景图" enumValues="0,1,2" default="2" desc="图片类型"/>
	</bgPic>	
</widget>