<widget name="pictureBox" type="widget" adapter="AImageAdapter"
			icon="widgets/picregion.png" >

     <categories>
     <category name = "basic"/>
     </categories>
     
	<properties>
		<property name="x" type="U16" value="" category="basic" editable="true" relative ="Left" desc="控件左上角x坐标"/>
		<property name="y" type="U16" value="" category="basic" editable="true" relative ="Top" desc="控件左上角y坐标"/>
		<property name="width" type="U16" value="100" category="basic" editable="false" desc="控件宽度"/>
		<property name="height" type="U16" value="80" category="basic" editable="false" desc="控件高度"/>
		<property name="visible" type="boolean" value="1" category="basic" editable="true" desc="控件是否显示"/>
		<property name="screenshotBgPic" type="Picture_BG" value="" category="basic" editable="false" desc="作为嵌入式背景图的背景图片"/>				
	</properties>
		
</widget>